{
    "name": "chrysanthos/password-history",
    "description": "Laravel password history",
    "keywords": [
        "Laravel",
        "Laravel Framework",
        "Laravel Package",
        "Laravel Auth",
        "Laravel Authentication",
        "Laravel Admin",
        "PHP Package",
        "Password History"
    ],
    "support": {
        "issues": "https://github.com/chrysanthos/password-history/issues",
        "source": "https://github.com/chrysanthos/password-history"
    },
    "homepage": "https://github.com/Chrysanthos/password-history",
    "license": "MIT",
    "authors": [
        {
            "name": "Chrysanthos Prodromou",
            "email": "pchrysanthos@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">7.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Chrysanthos\\PasswordHistory\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chrysanthos\\PasswordHistory\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chrysanthos\\PasswordHistory\\PasswordHistoryServiceProvider"
            ]
        }
    }
}
